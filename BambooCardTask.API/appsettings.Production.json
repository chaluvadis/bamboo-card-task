{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "log.log",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "OpenTelemetry"
            }
        ]
    },
    "Provider": {
        "Name": "FrankFurter"
    },
    "FrankFurter": {
        "BaseCurrency": "EUR",
        "ExchangeRateApiUrl": "https://api.frankfurter.dev/v1/",
        "ExcludedCurrencies": [
            "TRY",
            "PLN",
            "THB",
            "MXN"
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Jwt": {
        "Key": "prod_secret_key_change_me",
        "Issuer": "bamboocard.ae",
        "Audience": "bamboocard.ae"
    }
}