{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Information"
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Information",
                "BambooCardTask": "Debug"
            }
        },
        "WriteTo": [
            {
                "Name": "OpenTelemetry",
                "Args": {
                    "protocol": "grpc"
                }
            }
        ]
    },
    "Provider": {
        "Name": "FrankFurter"
    },
    "FrankFurter": {
        "BaseCurrency": "EUR",
        "ExchangeRateApiUrl": "https://api.frankfurter.dev/v1/",
        "ExcludedCurrencies": [
            "TRY",
            "PLN",
            "THB",
            "MXN"
        ]
    },
    "Jwt": {
        "Key": "prod_secret_key_change_me",
        "Issuer": "bamboocard.ae",
        "Audience": "bamboocard.ae"
    }
}